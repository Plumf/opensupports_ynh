{
    "name": "Open Supports",
    "id": "opensupports",
    "packaging_format": 1,
    "description": {
        "en": "$app is a ticket manager for your projects with the possibility to write documentation pages",
        "fr": "$app est un gestionnaire de ticket pour vos projets avec la possibilité de rédiger des pages de documentations"
    },
    "version": "4.10~ynh1",
    "url": "https://www.opensupports.com/",
    "upstream": {
        "license": "GPL-3.0-or-later",
        "website": "https://www.opensupports.com/",
        "demo": "https://www.opensupports.com/demo/",
        "admindoc": "https://docs.opensupports.com/",
        "userdoc": "https://docs.opensupports.com/",
        "code": "https://github.com/opensupports"
    },
    "license": "GPL-3.0-or-later",
    "maintainer": {
        "name": "Plumf",
        "email": "plumf@open-plug.eu"
    },
    "requirements": {
        "yunohost": ">= 4.3.0"
    },
    "multi_instance": true,
    "services": [
        "nginx",
        "php7.3-fpm",
        "mysql"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/opensupports",
                "default": "/opensupports"
            },
            {
                "name": "admin",
                "type": "user"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            },
            {
                "name": "language",
                "type": "string",
                "ask": {
                    "en": "Choose the application language",
                    "fr": "Choisissez la langue de l'application"
                },
                "choices": ["fr", "en"],
                "default": "fr"
            },
            {
                "name": "password",
                "type": "password",
                "help": {
                    "en": "Use the help field to add an information for the admin about this question.",
                    "fr": "Utilisez le champ aide pour ajouter une information à l'intention de l'administrateur à propos de cette question."
                }
            }
        ]
    }
}
